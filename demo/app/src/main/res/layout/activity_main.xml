<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:orientation="vertical"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">

    <Button
        android:id="@+id/openVid1Btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Open Video 21022"
        android:onClick="showVideo1"/>

    <Button
        android:id="@+id/openVid2Btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Open Video 30245"
        android:onClick="showVideo2"/>

    <Button
        android:id="@+id/openPGBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Open PG"
        android:onClick="showPG"/>

    <Button
        android:id="@+id/sendNewGetId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send New Get"
        android:onClick="sendNewGet"/>

    <Button
        android:id="@+id/getBanner"
        android:text="Load Banner 9549"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="loadBanner"/>

    <Button
        android:id="@+id/getTag"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Load 3rd party tag 10213"
        android:onClick="loadTag"/>

    <tv.superawesome.sdk.views.SABannerAd
        android:id="@+id/myBannerAd1"
        android:layout_width="match_parent"
        android:layout_height="500px" />

    <!--<fragment-->
        <!--android:id="@+id/myBannerAd1"-->
        <!--android:name="tv.superawesome.sdk.views.SABannerAd"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="500px" />-->

</LinearLayout>
